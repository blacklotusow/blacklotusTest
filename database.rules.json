{
  "rules": {
    "drafts": {
      "$uid": {
        ".read": "true",
        ".write": "auth.uid == $uid"
      }
    },
    "displayNames": {
      "$displayName": {
        ".write": "auth.uid == newData.val()"
      }
    },
    "articles": {
      "$uid": {
        ".write": "auth.uid == $uid"
      }
    }
  }
}
